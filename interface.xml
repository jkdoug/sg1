  <alias
   name="ignore_illusions__"
   match="^\s*ig\s+(\w+)\s*$"
   enabled="y"
   expand_variables="y"
   group="Interface"
   regexp="y"
   send_to="12"
   ignore_case="y"
   sequence="100"
  >
  <send>main.ignore("%1")</send>
  </alias>
  <alias
   name="ignore_report__"
   match="^\s*ig\s*$"
   enabled="y"
   expand_variables="y"
   group="Interface"
   regexp="y"
   send_to="12"
   ignore_case="y"
   sequence="100"
  >
  <send>main.ignore()</send>
  </alias>
  <alias
   name="unignore_illusions__"
   match="^\s*unig\s+(\w+)\s*$"
   enabled="y"
   expand_variables="y"
   group="Interface"
   regexp="y"
   send_to="12"
   ignore_case="y"
   sequence="100"
  >
  <send>main.unignore("%1")</send>
  </alias>
  <alias
   name="unignore_all__"
   match="^\s*unig\s*$"
   enabled="y"
   expand_variables="y"
   group="Interface"
   regexp="y"
   send_to="12"
