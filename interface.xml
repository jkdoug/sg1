  <alias
   name="reset_wield__"
   match="^\s*reset\s+wield\s*$"
   enabled="y"
   group="Interface"
   regexp="y"
   send_to="12"
   ignore_case="y"
   sequence="100"
  >
  <send>gear.rewield = {}
display.Info("Wielding reset")</send>
  </alias>
  <alias
   name="reset_arena__"
   match="^\s*reset\s+arena\s*$"
   enabled="y"
   group="Interface"
   regexp="y"
   script="main.arena_leave"
   ignore_case="y"
   sequence="100"
  >
  </alias>

  <alias
   enabled="y"
   group="Interface"
   keep_evaluating="y"
   match="^OnGMCP\s+Core\.Goodbye"
   name="quit_reset__"
   regexp="y"
   send_to="12"
   sequence="1"
  >
  <send>affs.reset()
bals.reset()
defs.reset()
flags.reset()
todo.reset()
wounds.reset()</send>
  </alias>

  <alias
   name="warning_danger__"
   match="^\s*OnDanger\s+(.+?)\s*$"
   enabled="y"
   group="Interface"
   regexp="y"
   script="main.danger"
   ignore_case="y"
   sequence="100"
  >
  </alias>
  <alias
   name="warning_instakill__"
