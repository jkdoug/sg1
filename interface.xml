   match="^\s*pause\s*$"
   enabled="y"
   group="Interface"
   regexp="y"
   script="main.pause"
   ignore_case="y"
   sequence="100"
  >
  </alias>
  <alias
   name="unpause__"
   match="^\s*unpause\s*$"
   enabled="y"
   group="Interface"
   regexp="y"
   script="main.unpause"
   ignore_case="y"
   sequence="100"
  >
  </alias>

  <alias
   name="debug_switches__"
   match="^\s*debug\s+(actions|affs|bals|bash|beast|cures|debate|defs|enemy|failsafes|flags|gear|gmcp|iff|map|scan|todo|wounds)\s+(on|off)\s*$"
   enabled="y"
   group="Interface"
   regexp="y"
   script="display.debug_switch"
   ignore_case="y"
   sequence="100"
  >
  </alias>
  <alias
   name="debug__"
   match="^\s*debug\s+(on|off)\s*$"
   enabled="y"
   group="Interface"
   regexp="y"
   script="display.debug_toggle"
   ignore_case="y"
   sequence="100"
  >
  </alias>
  <alias
   name="debug_summary__"
   match="^\s*debug(?:\s+summary)?\s*$"
   enabled="y"
   group="Interface"
   regexp="y"
   script="display.debug_summary"
   ignore_case="y"
   sequence="100"
  >
  </alias>

  <alias
   keep_evaluating="y"
   name="info_update__"
   match="^OnInfoUpdate"
   enabled="y"
   group="Interface"
   regexp="y"
   sequence="100"
  >
  <send></send>
  </alias>

  <alias
   name="reset__"
   match="^\s*reset\s*$"
   enabled="y"
   group="Interface"
   regexp="y"
   send_to="10"
   ignore_case="y"
   sequence="100"
  >
  <send>reset affs
reset bals
reset defs
reset failsafes
reset flags
reset todo
reset wield
reset wounds</send>
  </alias>
  <alias
   name="reset_affs__"
   match="^\s*reset\s+affs\s*$"
   enabled="y"
   group="Interface"
   regexp="y"
   send_to="12"
   ignore_case="y"
   sequence="100"
  >
  <send>affs.reset()
display.Info("Afflictions reset")</send>
  </alias>
  <alias
   name="reset_bals__"
   match="^\s*reset\s+bals\s*$"
   enabled="y"
   group="Interface"
   regexp="y"
   send_to="12"
