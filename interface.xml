<aliases>
  <alias
   name="affs__"
   match="^\s*affs\s*$"
   enabled="y"
   group="Interface"
   regexp="y"
   script="affs.show"
   ignore_case="y"
   sequence="100"
  >
  </alias>
  <alias
   name="flags__"
   match="^\s*flags\s*$"
   enabled="y"
   group="Interface"
   regexp="y"
   script="flags.show"
   ignore_case="y"
   sequence="100"
  >
  </alias>
  <alias
   name="failsafes__"
   match="^\s*failsafes?\s*$"
   enabled="y"
   group="Interface"
   regexp="y"
   script="failsafe.show"
   ignore_case="y"
   sequence="100"
  >
  </alias>
  <alias
   name="autos__"
   match="^\s*autos?\s*$"
   enabled="y"
   group="Interface"
   regexp="y"
