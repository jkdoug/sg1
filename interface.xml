   ignore_case="y"
   sequence="100"
  >
  </alias>
  <alias
   name="debug_gmcp__"
   match="^OnGMCP\s+(.+?)\s+(.+?)$"
   enabled="n"
   group="Interface"
   keep_evaluating="y"
   regexp="y"
   script="display.debug_gmcp"
   ignore_case="y"
   sequence="100"
  >
