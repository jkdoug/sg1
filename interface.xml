   ignore_case="y"
   sequence="100"
  >
  </alias>
  <alias
   name="auto_toggle__"
   match="^\s*auto\s+(\w+)\s+(on|off|nil)\s*$"
   enabled="y"
   group="Interface"
   regexp="y"
   script="main.auto_toggle"
   ignore_case="y"
   sequence="101"
  >
  </alias>
  <alias
   name="auto_toggle_tea__"
   match="^\s*auto\s+(tea)\s+(green|black|oolong|white|teapot)\s*$"
   enabled="y"
   group="Interface"
   regexp="y"
   script="main.auto_toggle"
   ignore_case="y"
   keep_evaluating="y"
   sequence="100"
  >
  </alias>
  <alias
   name="auto_toggle_imbue__"
