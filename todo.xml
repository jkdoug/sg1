<aliases>
  <alias
   name="todo_add__"
   match="^\s*do((?:free)?)(1?)\s+(.+?)\s*$"
   enabled="y"
   group="Interface"
   regexp="y"
   script="todo.handle_add"
   ignore_case="y"
   sequence="10000"
  >
  </alias>
  <alias
   name="todo_del__"
   match="^\s*undo(1?)\s+(.+?)\s*$"
   enabled="y"
   group="Interface"
   regexp="y"
   script="todo.handle_del"
   ignore_case="y"
   sequence="10000"
  >
  </alias>
  <alias
   name="todo_reset__"
   match="^\s*reset\s+todo\s*$"
   enabled="y"
   group="Interface"
   regexp="y"
   script="todo.handle_reset"
   ignore_case="y"
   sequence="100"
  >
  </alias>
  <alias
   name="todo_exec__"
   match="^\s*todo\s*$"
   enabled="y"
   group="Interface"
   regexp="y"
   script="todo.handle_exec"
   ignore_case="y"
   sequence="100"
  >
  </alias>
  <alias
   name="todo_show__"
   match="^\s*do\s*$"
   enabled="y"
   group="Interface"
   regexp="y"
   script="todo.handle_show"
   ignore_case="y"
   sequence="10000"
  >
  </alias>
</aliases>
